{
  "name": "scout-journal",
  "description": "Blog for Scout Upstate",
  "version": "1.0.0",
  "author": "Chris Ogden <ogdenchris@gmail.com>",
  "dependencies": {
    "gatsby": "^2.0.118",
    "gatsby-cli": "^2.4.9",
    "gatsby-link": "^2.0.9",
    "gatsby-plugin-feed": "^2.0.11",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sharp": "^2.0.18",
    "gatsby-plugin-styled-jsx": "^3.0.4",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.2.2",
    "gatsby-remark-responsive-iframe": "^2.0.8",
    "gatsby-remark-smartypants": "^2.0.7",
    "gatsby-source-filesystem": "^2.0.17",
    "gatsby-transformer-remark": "^2.2.0",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-responsive-grid": "^0.3.4",
    "serve": "^10.1.1",
    "styled-jsx": "^3.2.1",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^1.2.0",
    "prettier": "^1.16.1"
  },
  "scripts": {
    "dev": "gatsby develop",
    "now-build": "npm run build",
    "now-dev": "gatsby develop -p $PORT",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build --prefix-paths",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "start": "serve public/"
  }
}
